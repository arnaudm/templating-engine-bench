<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Friends</title>
</head>
<body>
    <div class="friends">
        {{#each friends}}
        <div class="friend">
            <ul>
                <li>Name: {{getFullNameDust}}</li>
                <li>
                    Balance: {{balance}}
                </li>
                <li>
                    Age: {{age}}
                </li>
                <li>
                    Address: {{address}}
                </li>
                <li>
                    Image: <img src="{{picture}}">
                </li>
                <li>
                    Company: {{company}}
                </li>
                <li>
                    Email: <a href="mailto:{{email}}">{{email}}</a>
                </li>
                <li>
                    About: {{about}}
                </li>
                {{#if tags}}
                <li>
                    Tags:
                    <ul>
                        {{#each tags}}
                        <li>
                            {{this}}
                        </li>
                        {{/each}}
                    </ul>
                </li>
                {{/if}}
            </ul>
        </div>
        {{/each}}
    </div>
</body>
</html>